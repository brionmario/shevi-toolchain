    "eslint": "^8.57.0",
    "eslint-config-turbo": "^1.13.3",
    "prettier": "^3.2.5",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  }
